<div id="<%= dom_id pet %>">
  <p>
    <strong>Category:</strong>
    <%= pet.category_id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= pet.name %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= pet.price %>
  </p>

  <p>
    <strong>Age:</strong>
    <%= pet.age %>
  </p>

  <p>
    <strong>Weight:</strong>
    <%= pet.weight %>
  </p>

  <p>
    <strong>Date of birth:</strong>
    <%= pet.date_of_birth %>
  </p>

  <p>
    <strong>Adopted at:</strong>
    <%= pet.adopted_at %>
  </p>

  <p>
    <strong>Wakeup time:</strong>
    <%= pet.wakeup_time %>
  </p>

  <p>
    <strong>Desexed:</strong>
    <%= pet.desexed %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= pet.description %>
  </p>

</div>
